{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
    },
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  
    "rules": {
      "no-unused-vars": "error",
      "no-unused-expression": "error",
      "prefer-const": "error",
      "no-undef": "error",
      "no-console": "warn"
    },
  
    "env": {
      "browser": true,
      "es2021": true
    },
    "globals":{
      "process": "readonly",
    }
  }